<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<Pane xmlns:fx="http://javafx.com/fxml/1" fx:id="background" prefHeight="836.0" prefWidth="704.0"
      xmlns="http://javafx.com/javafx/8.0.121" fx:controller="sample.Controller">


    <ImageView fx:id="chest" fitHeight="64.0" fitWidth="64.0" layoutX="128.0" layoutY="128.0" pickOnBounds="true"
