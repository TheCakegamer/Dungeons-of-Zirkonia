            <Image url="@../image/available.png"/>

        </ImageView>
        <ImageView fx:id="fields" fitHeight="64.0" fitWidth="64.0" onKeyTyped="#move" opacity="0.5"
                   pickOnBounds="true" preserveRatio="true" translateZ="1.0" GridPane.columnIndex="1"
                   GridPane.rowIndex="2">

            <Image url="@../image/available.png"/>

        </ImageView>
        <ImageView fx:id="actioncloud" fitHeight="64.0" fitWidth="64.0" opacity="0.59" pickOnBounds="true"
                   preserveRatio="true" visible="false">

            <Image url="@../image/actioncloud.gif"/>

        </ImageView>
    </GridPane>
    <ProgressBar fx:id="healthbar" layoutX="133.0" layoutY="724.0" minHeight="8.0" minWidth="393.0"
                 prefHeight="20.0" prefWidth="399.0" progress="1.0" style="-fx-accent: #ab343a; ">
    </ProgressBar>
    <ImageView fitHeight="141.0" fitWidth="704.0" pickOnBounds="true" preserveRatio="true" y="704.0">

        <Image url="@../image/statusbar.png"/>

    </ImageView>
    <ImageView fx:id="fearmeter" fitHeight="96.0" fitWidth="96.0" pickOnBounds="true" preserveRatio="true" x="8.0"
